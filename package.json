{
  "name": "@aureooms/js-list-spec",
  "description": "List ADT specification code bricks for JavaScript",
  "version": "9.0.1",
  "license": "AGPL-3.0",
  "author": "aureooms",
  "homepage": "https://make-github-pseudonymous-again.github.io/js-list-spec",
  "repository": {
    "url": "https://github.com/make-github-pseudonymous-again/js-list-spec",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/make-github-pseudonymous-again/js-list-spec/issues"
  },
  "keywords": [
    "bricks",
    "javascript",
    "js",
    "list",
    "specification"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "build-docs": "esdoc",
    "build-gh-pages": "npm run build-docs",
    "commit-msg": "commitlint --edit",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "install-hooks": "husky install",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "npm run install-hooks",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "release": "np",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "@commitlint/cli": "17.2.0",
    "@js-library/commitlint-config": "0.0.4",
    "ava": "3.8.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.2.0",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.12.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "husky": "8.0.1",
    "np": "6.2.3",
    "pinst": "3.0.0",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.10",
    "xo": "0.52.4"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": [
            "defaults",
            "maintained node versions"
          ]
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-async-generator-functions"
    ],
    "env": {
      "cover": {},
      "development": {
        "sourceMaps": "inline"
      }
    }
  }
}
